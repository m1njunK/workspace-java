package programmers;

import java.util.Arrays;
import java.util.HashSet;
import java.util.Set;

public class Nn {
	public static void main(String[] args) {

		int[] people = {70, 50, 80};
		int limit = 100;
		
		Arrays.sort(people);
		
		System.out.println(Arrays.toString(people));
		
		int sum = 0;
		int cnt = 0;
		
		for(int i : people) {
			if(sum + i >= limit) {
				cnt++;
			}
			sum += i;
		}
		
		System.out.println(cnt);
		
	} //
} //
